{
  "name": "zustand-with-reset",
  "version": "1.0.0",
  "description": "Utility functions for creating Zustand stores with automatic reset functionality",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.mjs",
        "require": "./dist/index.js",
        "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "vite build",
    "prepublishOnly": "npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "dev": "vite build --watch"
  },
  "keywords": [
    "zustand",
    "state-management",
    "react",
    "store",
    "reset"
  ],
  "author": "Mojahidul Islam",
  "license": "MIT",
  "peerDependencies": {
    "zustand": "^4.0.0"
  },
  "devDependencies": {
    "tsup": "^7.2.0",
    "typescript": "^5.0.2",
    "vitest": "^3.1.1",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.6.0",
    "zustand": "^4.5.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/itsemon245/zustand-store-utils.git"
  },
  "bugs": {
    "url": "https://github.com/itsemon245/zustand-store-utils/issues"
  },
  "homepage": "https://github.com/itsemon245/zustand-store-utils#readme"
}
